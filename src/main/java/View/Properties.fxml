<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="mainAnchor" prefHeight="299.0" prefWidth="535.0" stylesheets="@Properties.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.PropertiesController">
    <children>
        <MenuButton fx:id="mazeAlgo" layoutX="14.0" layoutY="59.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="245.0" styleClass="properties" text="My Maze Generator">
            <items>
                <MenuItem fx:id="SimpleMaze" mnemonicParsing="false" onAction="#mouseClicked" styleClass="properties" text="Simple Maze Generator" />
                <MenuItem fx:id="EmptyMaze" mnemonicParsing="false" onAction="#mouseClicked" styleClass="properties" text="Empty Maze Generator" />
                <MenuItem fx:id="MyMaze" mnemonicParsing="false" onAction="#mouseClicked" styleClass="properties" text="My Maze Generator" />
            </items>
         <font>
            <Font name="Gabriola" size="20.0" />
         </font>
        </MenuButton>
        <MenuButton fx:id="searchingAlgo" layoutX="14.0" layoutY="150.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="245.0" styleClass="properties" text="Best First Search">
            <items>
                <MenuItem fx:id="Best" mnemonicParsing="false" onAction="#mouseClicked" styleClass="properties" text="Best First Search" />
                <MenuItem fx:id="BFS" mnemonicParsing="false" onAction="#mouseClicked" styleClass="properties" text="Breadth First Search" />
                <MenuItem fx:id="DFS" mnemonicParsing="false" onAction="#mouseClicked" styleClass="properties" text="Depth First Search" />
            </items>
         <font>
            <Font name="Gabriola" size="20.0" />
         </font>
        </MenuButton>
        <Label layoutX="23.0" layoutY="29.0" prefHeight="17.0" prefWidth="122.0" text="Maze Generator" textFill="WHITE">
         <font>
            <Font name="Gabriola" size="24.0" />
         </font></Label>
        <Label layoutX="21.0" layoutY="120.0" prefHeight="30.0" prefWidth="210.0" text="Maze Searching Algorithm" textFill="WHITE">
         <font>
            <Font name="Gabriola" size="24.0" />
         </font></Label>
        <Button fx:id="applyChanges" layoutX="126.0" layoutY="242.0" mnemonicParsing="false" onAction="#mouseClicked" prefHeight="43.0" prefWidth="128.0" styleClass="properties" text="Apply Changes">
         <font>
            <Font name="Gabriola" size="20.0" />
         </font></Button>
        <Button fx:id="cancel" layoutX="14.0" layoutY="242.0" mnemonicParsing="false" onAction="#mouseClicked" prefHeight="25.0" prefWidth="91.0" styleClass="properties" text="Cancel">
         <font>
            <Font name="Gabriola" size="22.0" />
         </font></Button>
    </children>
</AnchorPane>
