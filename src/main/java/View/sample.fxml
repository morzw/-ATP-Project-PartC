              </menus>
            </MenuBar>
         </top>
         <bottom>
            <MenuButton mnemonicParsing="false" text="MenuButton" BorderPane.alignment="CENTER">
              <items>
                <MenuItem mnemonicParsing="false" text="Action 1" />
